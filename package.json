{
  "name": "alt-ng",
  "version": "0.0.2",
  "description": "A new generation of the alt flux implementation",
  "main": "Alt.js",
  "jsnext:main": "Alt.js",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.6.0",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "sinon": "^1.17.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koliseoapi/alt-ng.git"
  },
  "authors": [
    "Carlos Coloma <ccescribano@gmail.com>",
    "Nacho Coloma <icoloma@gmail.com>"
  ],
  "license": "MIT",
  "scripts": {
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --reporter=lcov --reporter=text node_modules/.bin/_mocha --compilers js:babel-register test",
    "postversion": "git push && git push --tags",
    "test": "node_modules/.bin/_mocha --compilers js:babel-register test"
  },
  "files": [
    "src",
    "Alt.js",
    "Store.js",
    "AltContainer.js"
  ],
  "keywords": [
    "alt",
    "es6",
    "flux",
    "react",
    "alt"
  ],
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
